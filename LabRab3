using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace LabRab3
{
    class Program
    {
        static void Main(string[] args)
        {
            int x, y;
            bool drKvadrant;
            Console.WriteLine("Enter x: ");
            x = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Enter y: ");
            y = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Does this x and y are the coordinates of the second quadrant:");
            if (x < 0 && y > 0)
            {
                drKvadrant = true;
            }
            else
            {
                drKvadrant = false;
            }
            if (drKvadrant == true)
            {
                Console.WriteLine("True");
            }
            else
            {
                Console.WriteLine("False");
            }
            Console.ReadKey();
        }
    }
}
